{
    "name": "nding/micky-framework",
    "description": "Framework MVC",
    "authors": [{
        "name": "Ndinga Mickael",
        "email": "mickydev@gmail.com"
    }],
    "require": {
        "guzzlehttp/guzzle": "^7.3",
        "symfony/dotenv": "^5.3",
        "nesbot/carbon": "^2.54",
        "minishlink/web-push": "^6.0",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "core/",
            "Tests\\": "tests/",
            "App\\": "app/",
            "DebugBar\\": "core/DebugBar/",
            "HTML\\": "core/HTML"
        },
        "files": [
            "core/Helpers/config.php",
            "core/Helpers/route.php",
            "core/Helpers/view.php",
            "core/Helpers/lang.php",
            "core/Helpers/generate.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.9",
        "symfony/var-dumper": "^5.3",
        "robmorgan/phinx": "^0.12.7",
        "fakerphp/faker": "^1.15",
        "mbezhanov/faker-provider-collection": "^2.0",
        "spatie/phpunit-watcher": "^1.23"
    }
}
